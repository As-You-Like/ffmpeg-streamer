{
  "name": "ffmpeg-streamer",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev": "NODE_ENV=development node --trace-warnings app.js",
    "dev-win": "set NODE_ENV=development&& node --trace-warnings app.js",
    "__postinstall": "npm run pkg",
    "pkg": "node node_modules/pkg/lib-es5/bin.js package.json --out-path dist --targets node8-macos-x64,node8-win-x64,node8-win-x86,node8-linux-x64,node8-linux-x86",
    "__pkgc": "npm run pkg && git commit -m 'build binaries' -- dist"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "ejs": "~2.5.7",
    "express": "^4.16.2",
    "ffbinaries": "^1.0.8",
    "ffmpeg-respawn": "0.0.12",
    "flv.js": "^1.4.0",
    "hls.js": "^0.8.9",
    "material-design-lite": "^1.3.0",
    "morgan": "~1.9.0",
    "mp4frag": "0.0.17",
    "pipe2jpeg": "^0.2.7",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.0.4"
  },
  "bin": "app.js",
  "main": "app.js",
  "devDependencies": {
    "pkg": "^4.3.0"
  },
  "pkg": {
    "assets": [
      "views/**/*",
      "public/**/*"
    ]
  }
}
