{
  "name": "ip-cam-tester",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "pkg": "pkg package.json --out-path dist --targets node8-macos-x64,node8-macos-x86,node8-win-x64,node8-win-x86,node8-linux-x64,node8-linux-x86 && git commit -m 'build binaries' -- dist"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "^3.1.0",
    "ejs": "~2.5.7",
    "express": "^4.16.2",
    "ffmpeg-respawn": "0.0.6",
    "flv.js": "^1.4.0",
    "hls.js": "^0.8.9",
    "material-design-lite": "^1.3.0",
    "morgan": "~1.9.0",
    "mp4frag": "0.0.14",
    "pipe2jpeg": "^0.2.6",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.0.4"
  },
  "bin": "app.js",
  "main": "app.js",
  "devDependencies": {
    "electron": "^1.8.2"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "views/**/*",
      "public/**/*",
      "node_modules/ffmpeg-static/bin/darwin/x64/ffmpeg"
    ]
  }
}
